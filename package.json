{
  "name": "remix-gen",
  "version": "0.0.1",
  "packageManager": "yarn@3.2.2",
  "keywords": [
    "remix",
    "remix-run"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "package.json",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts",
    "prepublish": "yarn build"
  },
  "devDependencies": {
    "@remix-run/node": "^1.6.7",
    "tsup": "^6.2.1",
    "typescript": "^4.7.4"
  }
}
